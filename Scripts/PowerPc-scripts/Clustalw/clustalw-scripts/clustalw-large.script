#!/bin/bash 
source $HOME/.bioperf
cd $BIOPERF/Binaries/PowerPc-Binaries/Clustalw
./clustalw -batch -infile=$BIOPERF/Inputs/Clustalw/class-C/6000.seq
mv $BIOPERF/Inputs/Clustalw/class-C/6000.aln $BIOPERF/Inputs/Clustalw/class-C/6000.dnd  $BIOPERF/Outputs/Clustalw/
echo The output has been stored as 6000.dnd and 6000.aln in /Outputs/Clustalw

